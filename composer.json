{
    "name": "tightenco/takeout",
    "description": "Manage your dev dependencies with simple one-off Docker containers.",
    "keywords": ["cli", "console", "docker", "containers", "dependency management"],
    "homepage": "https://takeout.tighten.co/",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Matt Stauffer",
            "email": "matt@tighten.co"
        },
        {
            "name": "Jose Soto",
            "email": "jose@tighten.co"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "ext-pcntl": "*",
        "ext-posix": "*",
        "composer/semver": "^3.4",
        "guzzlehttp/psr7": "^2.6"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^7.5",
        "laravel-zero/framework": "^11.0",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/laravel-console-menu": "^3.4",
        "phpunit/phpunit": "^10.5",
        "squizlabs/php_codesniffer": "^3.5",
        "tightenco/tlint": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform-check": false
    },
    "scripts": {
        "lint:check": [
            "tlint",
            "phpcs"
        ],
        "lint:fix": [
            "phpcbf"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["builds/takeout"]
}
